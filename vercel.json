{
	"devCommand": "npm run dev",
	"builds": [
		{
			"src": "src/server.ts",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "/src/server.ts",
			"methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
			"headers": {
				"Access-Control-Allow-Origin": "https://watcher-owl.vercel.app/"
			}
		}
	]
}
